<template>
    <div class="wrap" style="width:750;height:{{sHeight}};background-color:#f6f6f6">

      <div class="my-wave">
        <text class="my-wave-text">我的浪花：</text>
        <image class="my-wave-image" quality="high" src="{{waveImage}}"></image>
        <text class="my-wave-coin">{{waveNumber}}</text>
      </div>

      <div if={{isPayInfoShown}} style="flex:1;">
        <div class="divider"></div>

        <clickview class="pay-channel-item" id="item-alipay" if={{isAlipayShown}} onclick="alipayClick">
          <div class="pay-channel-info">
            <image class="pay-channel-img" quality="high" src="{{alipayImage}}"></image>
            <text class="pay-channel-text">支付宝</text>
          </div>
          <image class="pay-channel-check" quality="high" id="check-alipay" src="{{checkImage}}"></image>
        </clickview>

        <div class="divider-short" if={{isChannelDividerShown}}></div>

        <clickview class="pay-channel-item" id="item-wechatpay" if={{isWechatShown}} onclick="wechatClick">
          <div class="pay-channel-info">
            <image class="pay-channel-img" quality="high" src="{{wechatImage}}"></image>
            <text class="pay-channel-text">微信支付</text>
          </div>
          <image class="pay-channel-check" quality="high" id="check-wechat" src="{{uncheckImage}}"></image>
        </clickview>


        <div class="recharge-title">
          <text class="recharge-title-text">充 值</text>
        </div>

        <list style="width:750;height:{{listHeight}};">
          <cell repeat="{{payDiamondInfoList}}">
            <clickview class="pay-item" index="{{$index}}" onclick="payItemClick">
              <div class="pay-item-info">
                <image class="pay-wave-img" quality="high" src="{{waveImage}}"></image>
                <text class="pay-wave-number">{{buy_diamond_num}}</text>
              </div>
              <!-- <div class="btn-pay">
                <text class="btn-pay-text">¥ {{amount}}</text>
              </div> -->
              <loading-button class="pay-loading-btn" id="loading-{{$index}}" buttontext="¥ {{amount}}"></loading-button>
            </clickview>
            <div class="divider-short" if={{isDividerShown($index)}}></div>
          </cell>
        </list>

      </div>


      <!-- <div class="network-error" style="height:{{networkErrorDivHeight}}" if={{isNetworkErrorShown}}>
        <div class="network-error-content" onclick="networkErrClick">
          <image class="network-error-img" quality="high" src="{{networkErrorImage}}"></image>
          <text class="network-error-text">网络不给力，请重新加载</text>
        </div>
      </div> -->

      <div if={{isNetworkErrorShown}}>
        <swiperefresh id="network-error-refresh" style="height:{{networkErrorDivHeight}}" color="blue" eventname="payRefresh" refreshing="false">
          <scroller id="network-error" class="network-error"  style="height:{{networkErrorDivHeight}}">
            <div class="network-error-content" onclick=""  >
              <image class="network-error-img" quality="high" src="{{networkErrorImage}}"></image>
              <text class="network-error-text">网络不给力，请重新加载</text>
            </div>
          </scroller>
        </swiperefresh>
      </div>

      <div class="init-loading" style="height:{{networkErrorDivHeight}}" if={{isInitLoadingWheelShown}}>
        <progresswheel class="init-loading-wheel" width="110" height="110" barwidth="4" barcolor="blue"></progresswheel>
      </div>


      <!-- <text>result: {{dataresult}}</text> -->
      <!-- <text>msg2: {{msg2}}</text> -->
      <!-- <text>msg3: {{msg3}}</text> -->
      <!-- <text>msg4: {{msg4}}</text> -->
      

    </div>
</template>

