  var modal = require('@weex-module/modal');
  var payModule = require('@weex-module/payModule');
  var utilModule = require('@weex-module/utilModule');
  var apis = require('../common/api');
  var globalEvent = require('@weex-module/globalEvent');
  var stream
  __weex_define__('@weex-temp/api', function (__weex_require__) {
      stream = __weex_require__('@weex-module/stream')
  });

  module.exports = {
    data: {
      sHeight: '',
      listHeight: '',

      current_rotate: 0,
      stop_rotate: 0,
      showCircle: false,
      loadingDuration: 1000,

      //user info
      appType: '3',
      ctoken: '',
      utoken: '',
      network: '',
      version: '',
      deviceId: '',
      plat: 'Android',
      productType: 'EMLive',
      reqtype: 'server',

      wechatTradeNo: '',
      clickIndex: -1,

      waveNumber: 0,

      isPayInfoShown: false,
      isNetworkErrorShown: false,
      networkErrorDivHeight: 0,
      isInitLoadingWheelShown: true,
      refresh_display: 'hide',

      isAlipayShown: false,
      isWechatShown: false,
      isChannelDividerShown: false,

      isAlipay: true,
      isWechat: false,

      payDiamondInfoList: [],
      payDiamondInfoListCount: 0,

      // alipayImage: 'http://p1.bqimg.com/567571/e7a2ca5c31afc230.png',
      // wechatImage: 'http://p1.bqimg.com/567571/74d56ea546118e19.png',
      // waveImage: 'http://p1.bqimg.com/567571/125c104519d42f02.png',
      // checkImage: 'http://p1.bpimg.com/567571/c5b8336ca3347ebf.png',
      // uncheckImage: 'http://p1.bpimg.com/567571/16b239a8d71f678f.png',
